<h1>Add new place</h1>


<%= form_with model: @place do |form| %>
    <p>
    <%= form.label "title", "Title" %>
    <%= form.text_field "title" %>
    </p>

    <p>
    <%= form.label "description" %>
    <%= form.text_field "description" %>
    </p>

    <p>
    <%= form.label "posted_on" %>
    <%= form.text_field "posted_on" %>
    </p>

  <%= form.submit %>
  
<% end %>

